  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Sofosbuvir</intervention_name>
    <description>Sofosbuvir (SOF) administered as a 400 mg tablet orally once daily</description>
    <arm_group_label>SOF+PEG+RBV 12 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 24 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 12 week/Rerandomization Group</arm_group_label>
    <other_name>Sovaldi®</other_name>
    <other_name>GS-7977</other_name>
    <other_name>PSI-7977</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>RBV</intervention_name>
    <description>Ribavirin (RBV) tablets administered orally in a divided daily dose according to package insert weight-based dosing recommendations (&lt; 75kg = 1000 mg and ≥ 75 kg = 1200 mg)</description>
    <arm_group_label>SOF+PEG+RBV 12 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 24 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 12 week/Rerandomization Group</arm_group_label>
    <other_name>Copegus®</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>PEG</intervention_name>
    <description>Pegylated interferon alfa-2a (PEG) 180 μg administered once weekly by subcutaneous injection</description>
    <arm_group_label>SOF+PEG+RBV 12 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 24 weeks</arm_group_label>
    <arm_group_label>SOF+PEG+RBV 12 week/Rerandomization Group</arm_group_label>
    <other_name>Pegasys®</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Males and females with Chronic Hepatitis C (HCV) Genotype 1,4,5,6, or indeterminate

          -  Naive to previous HCV treatment

        Exclusion Criteria:

          -  Positive for HBsAg, anti-HBc IgM Ab, or anti-HIV Ab

          -  History of any other clinically significant chronic liver disease
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
