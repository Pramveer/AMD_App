  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  adult patients, 18-70 years of age;

          -  chronic hepatitis B;

          -  HBV DNA &gt;100,000 copies/mL.

        Exclusion Criteria:

          -  previous antiviral or interferon-based therapy for chronic hepatitis B in past 6
             months;

          -  evidence of decompensated liver disease;

          -  history or evidence of a medical condition associated with chronic liver disease
             other than viral hepatitis;

          -  coinfection with hepatitis A, C or D, or HIV.
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
