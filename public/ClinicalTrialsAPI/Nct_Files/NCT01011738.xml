  <eligibility>
    <study_pop>
      <textblock>
        Patients with chronic hepatitis C receiving treatment with peginterferon alfa-2a 40KD
        (Pegasys)
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  adult patient, &gt;/= 18 years of age

          -  chronic hepatitis B

          -  treatment with peginterferon alfa-2A

        Exclusion Criteria:

          -  coinfection with HAV, HCV and HIV
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
